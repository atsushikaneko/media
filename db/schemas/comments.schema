# -*- mode: ruby -*-
# vi: set ft=ruby :
create_table "comments", force: :cascade do |t|
  t.text "content"
  t.integer "blog_id"
  t.integer "user_id"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.index ["blog_id"], name: "index_comments_on_blog_id"
  t.index ["user_id"], name: "index_comments_on_user_id"
end
